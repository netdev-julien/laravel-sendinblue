{
    "name": "netdev-julien/laravel-sendinblue",
    "description": "Laravel's mail transport for SendinBlue",
    "keywords": ["laravel", "laravel5", "lumen", "sendinblue", "smtp", "mailing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Ducatel",
            "email": "baptiste@agence-webup.com",
            "homepage": "http://agence-webup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/mail": "^5.5|^6.0",
        "guzzlehttp/guzzle": "^6.3",
        "sendinblue/api-v3-sdk": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Webup\\LaravelSendinBlue\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webup\\LaravelSendinBlue\\SendinBlueServiceProvider"
            ]
        }
    }
}
